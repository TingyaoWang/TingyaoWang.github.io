{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/TingyaoWang/TingyaoWang.github.io/main/data_MM23-K222.json",
      "format": {"type": "json", "property": "years"}
    },
    "title": {
      "text": "The Output in Production Industries",
      "subtitle": "From 1940-2020, Source: ONS",
      "subtitleFontStyle": "italic",
      "subtitleFontSize": 10,
      "anchor": "start",
      "color": "black"
    },
    "width": 200,
    "height": 180,
    "mark": {"type": "line", "color": "rgb(0,47,167"},
    "encoding": {
      "x": {
        "field": "year",
        "type": "temporal",
        "axis": {"grid": false},
        "title": null
      },
      "y": {
        "field": "value",
        "type": "quantitative",
        "axis": {"grid": false},
        "title": "The value of the output"
      }
    }
  }