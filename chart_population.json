{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "UK's Population ",
    "subtitle": "The population in the UK, 1971-2021 , Source: Office for National Statistics.",
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 10,
    "anchor": "start",
    "color": "black"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/TingyaoWang/TingyaoWang.github.io/main/population_data.csv"
  },
  "height": 300,
  "width": 300,
  "mark": {"type": "line", "color": "rgb(0,46,167"},
  "encoding": {
    "x": {
      "field": "Year",
      "timeUnit": "utcyear",
      "type": "nominal",
      "axis": {"title": null, "grid": false, "ticks": false, "labelAngle": 45}
    },
    "y": {
      "field": "Population",
      "type": "quantitative",
      "title": "",
      "axis": {"grid": false},
      "scale": {"domain": [50000000, 70000000]}
    }
  }
}
